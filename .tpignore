# ignore clearcase files
.*/lost+found/.*
.*/[^/]+\.copyarea.*
.*/[^/]+\.keep
.*/[^/]+\.loading
.*/[^/]+\.merge?
.*/[^/]+\harvest.sig

# ignore .class files
.*/[^/]+\.class

# ignore packaged files #
.*/[^/]+\.jar
.*/[^/]+\.war
.*/[^/]+\.ear
.*/[^/]+\.tar
.*/[^/]+\.tar.gz
.*/[^/]+\.tgz
.*/[^/]+\.rar
.*/[^/]+\.zip
.*/[^/]+\.exe

# ignore all target directories and nested content
.*/target/.*

# ignore all generated-sources directories and nested content
.*/generated-sources/.*

# ignore the eclipse .settings directory and all children
.*/\.settings/.*
# ignore other eclipse files as these aspects are managed by the Maven pom.xml
.*/\.classpath
.*/\.project

# ignore Angular 2 directories and content
.*/node_modules/.*
.*/dist/.*

# ignore Frameworks appCtx.properties file from the EAR deployment
#   this file will be generated into this location using the replacer-plugin when
#   updating the build information.
.*/APP-INF/classes/appCtx.properties

