<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="CucumberTests" parallel="methods" data-provider-thread-count="5">
	<listeners>
		<listener
			class-name="com.test.cucumber.listener.CucumberRetryListener" />
	</listeners>
	<test name="CucumberTests">
		<classes>
			<class name="com.telus.runners.AppCucumberRunner" />
		</classes>
	</test>
</suite> <!-- Suite -->
